{
  "name": "nova-sonic",
  "version": "1.0.0",
  "description": "Nova Sonic Bidirectional Stream API for Amazon Bedrock",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "start": "node dist/server-cli.js",
    "dev": "ts-node-dev --respawn src/server-cli.ts",
    "cli": "ts-node src/index-cli.ts",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "amazon",
    "bedrock",
    "nova-sonic",
    "voice",
    "ai",
    "streaming"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.787.0",
    "@aws-sdk/credential-providers": "^3.787.0",
    "@modelcontextprotocol/sdk": "^1.10.2",
    "@smithy/node-http-handler": "^4.0.4",
    "@smithy/types": "^4.2.0",
    "axios": "^1.12.0",
    "cors": "^2.8.5",
    "express": "^4.18.0",
    "playwright": "^1.51.1",
    "rxjs": "^7.8.2",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.0",
    "@types/express": "^4.17.0",
    "@types/node": "^18.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "prettier": "^2.8.0",
    "rimraf": "^4.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.0"
  }
}
